                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" SnapsToDevicePixels="True" HorizontalAlignment="Left">
                                        <Button Name="WingetConfig" Content="Set Winget Settings" Cursor="Hand" Margin="5,0" HorizontalAlignment="Left" FontSize="16" FontFamily="Helvetica" Style="{DynamicResource ButtonStyle}" Height="30" Width="220" ToolTip="Set progress bar to rainbow, default langauge to english, default install location for portable packages to [C:/Users/winget/Packages, C:/Program Files/winget/Packages], disable telemetry and log errors."/>
                                        <Button Name="FixesADB" Content="Set ADB Environment" Cursor="Hand" Margin="5,5" HorizontalAlignment="Left" FontSize="16" FontFamily="Helvetica" Style="{DynamicResource ButtonStyle}" Height="30" Width="220"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" SnapsToDevicePixels="True" HorizontalAlignment="Left">
                                        <Label Content="Activate Windows:" FontSize="18" FontWeight="Bold" HorizontalAlignment="Left" Foreground="#FFDEE39B"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" SnapsToDevicePixels="True" HorizontalAlignment="Left">
                                        <Button Name="ActivateWindows" Content="Run Microsoft Activation Scripts (MAS)" Cursor="Hand" Margin="5,5" HorizontalAlignment="Left" FontSize="16" FontFamily="Helvetica" Style="{DynamicResource ButtonStyle}" Height="30" Width="450" ToolTip="A Windows and Office activator using HWID / Ohook / KMS38 / Online KMS activation methods, with a focus on open-source code and fewer antivirus detections."/>
                                    </StackPanel>
                                </WrapPanel>
