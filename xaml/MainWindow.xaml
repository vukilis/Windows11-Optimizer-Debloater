                        </Border>
                        <Border Margin="5" Height="40" CornerRadius="7" Background="#1f1f21" BorderBrush="#FFF4AB51" BorderThickness="1" SnapsToDevicePixels="True" Grid.Row="0" Grid.Column="2">
                            <StackPanel Background="#1f1f21" SnapsToDevicePixels="True" Margin="3">
                                <Label Content="Features" FontSize="18"  Foreground="#FFF4AB51" HorizontalAlignment="Center" Style="{StaticResource chfont}" FontWeight="Bold"/>
                            </StackPanel>
                        </Border>
                        <Border Margin="5" CornerRadius="7" Background="#1f1f21" BorderBrush="#FFF4AB51" BorderThickness="1" SnapsToDevicePixels="True" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="1">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <StackPanel Margin="30,30" SnapsToDevicePixels="True" HorizontalAlignment="Center">
                                    <CheckBox Name="FeaturesDotnet" Width="auto" Content="All .Net Framework (2,3,4)" Cursor="Hand" HorizontalAlignment="Left" Margin="8,5" FontSize="11" FontFamily="Gadugi" Foreground="#a69f6c">
                                        <CheckBox.LayoutTransform>
                                            <ScaleTransform ScaleX="1.5" ScaleY="1.5"/>
                                        </CheckBox.LayoutTransform>
                                    </CheckBox>
                                    <CheckBox Name="FeaturesHyperv" Width="auto" Content="HyperV Virtualization" Cursor="Hand" Margin="8,5" HorizontalAlignment="Left" FontSize="11" FontFamily="Gadugi" Foreground="#a69f6c">
                                        <CheckBox.LayoutTransform>
                                            <ScaleTransform ScaleX="1.5" ScaleY="1.5"/>
                                        </CheckBox.LayoutTransform>
                                    </CheckBox>
                                    <CheckBox Name="FeaturesLegacymedia" Width="auto" Content="Legacy Media (WMP, DirectPlay)" Cursor="Hand" HorizontalAlignment="Left" Margin="8,5" FontSize="11" FontFamily="Gadugi" Foreground="#a69f6c">
                                        <CheckBox.LayoutTransform>
                                            <ScaleTransform ScaleX="1.5" ScaleY="1.5"/>
                                        </CheckBox.LayoutTransform>
                                    </CheckBox>
                                    <CheckBox Name="FeatureNfs" Width="auto" Content="NFS - Network File System" Cursor="Hand" HorizontalAlignment="Left" Margin="8,5" FontSize="11" FontFamily="Gadugi" Foreground="#a69f6c">
                                        <CheckBox.LayoutTransform>
                                            <ScaleTransform ScaleX="1.5" ScaleY="1.5"/>
                                        </CheckBox.LayoutTransform>
                                    </CheckBox>
                                    <CheckBox Name="FeatureWsl" Width="auto" Content="Windows Subsystem for Linux" Cursor="Hand" HorizontalAlignment="Left" Margin="8,5" FontSize="11" FontFamily="Gadugi" Foreground="#a69f6c">
                                        <CheckBox.LayoutTransform>
                                            <ScaleTransform ScaleX="1.5" ScaleY="1.5"/>
                                        </CheckBox.LayoutTransform>
                                    </CheckBox>
                                    <CheckBox Name="FeatureSandbox" Width="auto" Content="Windows Sandbox" Cursor="Hand" HorizontalAlignment="Left" Margin="8,5" FontSize="11" FontFamily="Gadugi" Foreground="#a69f6c">
                                        <CheckBox.LayoutTransform>
                                            <ScaleTransform ScaleX="1.5" ScaleY="1.5"/>
                                        </CheckBox.LayoutTransform>
                                    </CheckBox>
                                    <Button Name="FeatureInstall" Content="Install Features" Cursor="Hand" FontSize="16" FontFamily="Helvetica" Style="{DynamicResource ButtonStyle}" Height="40" Width="190" Margin="0,10,0,10" Padding="10" HorizontalAlignment="Center"/>
                                    <Button Name="PanelAutologin" Content="Set Up Autologin" Cursor="Hand" FontSize="16" FontFamily="Helvetica" Style="{DynamicResource ButtonStyle}" Height="40" Width="190" Margin="0,10,0,10" Padding="10" HorizontalAlignment="Center"/>
                                </StackPanel>
                            </Grid>
                        </Border>
                    </Grid>
                </TabItem>
            </TabControl>
        </ScrollViewer>
        <StatusBar Name="ElevatorStatus"  DockPanel.Dock="Bottom" VerticalAlignment="Bottom" Grid.Row="2" Background="Transparent" Visibility="Collapsed">
            <StatusBarItem Name="ElevatorMode" Content="" FontWeight="Bold" FontSize="16"/>
        </StatusBar>
    </Grid>
</Window>